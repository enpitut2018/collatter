<p id="notice"><%= notice %></p>

<h1>テンプレート一覧</h1>

<div class="collas">
<% @templates.each do |template| %>
<div>
  <%= link_to template, target: "_brank" do %>
    <%= image_tag template.image.to_s %>
    <br><%= template.tag_txt %>
  <% end %>
  <br><%= link_to '削除', template, method: :delete, data: { confirm: 'Are you sure?' } %>
</div>
<% end %>
</div>
